{
  "name": "gtr-test-xray",
  "version": "1.0.0",
  "description": "A WebdriverIO-based test testing library for the guitar practice app.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcypherthis/gtr-test-xray.git"
  },
  "homepage": "https://guitar-practice-app.herokuapp.com/",
  "bugs": {
    "url": "http://github.com/dcypherthis/gtr-test-xray",
    "email": "jcypher@myemma.com"
  },
  "contributors": [
    "Josh Cypher <jcypher@myemma.com> (http://github.com/dcypherthis/)",
    "Jackson Streeter <jstreeter@myemma.com> (http://github.com/streeter8/)"
  ],
  "license": "MIT",
  "main": "config/master.conf.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src -d lib",
    "lint": "eslint ./*",
    "node-inspector": "node-inspector --debug-port 5859 --no-preload",
    "build-docs": "jsdoc -c ./docs/config/jsdoc.conf.json -t node_modules/ink-docstrap/template/ -R README.md -r src -d docs/build",
    "show-docs": "sh docs/config/openDocs.sh"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "6.14.0",
    "babel-register": "^6.14.0",
    "chai": "3.5.0",
    "config": "^1.21.0",
    "cucumber": "^1.2.0",
    "deepmerge": "^0.2.10",
    "node-uuid": "^1.4.7",
    "request": "^2.74.0",
    "wdio-cucumber-framework": "0.2.5",
    "wdio-dot-reporter": "0.0.6",
    "wdio-junit-reporter": "^0.1.0",
    "wdio-sauce-service": "^0.2.5",
    "wdio-spec-reporter": "0.0.3",
    "webdriverio": "^4.2.16"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "eslint": "^3.4.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.2"
  }
}
